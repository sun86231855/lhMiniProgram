.Position{ color: #363636; font-size: 24rpx }
.nav_section_items .section_cont .section_cont_name{ font-size:26rpx; border-radius: 18px;
 width: 70%; margin: 0 15%; color: #333}
.nav_section_items .section_cont .section_cont_name .icon_vip_img{
   position: absolute; top: -5px; right: -10px; width: 16px; height: 16px;
}
.nav_section_items .section_cont .section_cont_name .icon_vip_img img{ width: 100%; height: 100%}
.nav_section_items .section_book{ display: none; width:80rpx; height:80rpx; font-size:26rpx; border-radius:100%; background:#d24a58; line-height: 80rpx; color:#fff; text-align: center; margin-top:35rpx;}
