/**group_details.wxss**/
/**color #e02e24**/
.hide{
 display: none;
}
.show{ 
 display: flex;
}
.container{ padding:20rpx 2%; margin:0rpx;}
.header,
.master,


.people,
.href {
  width: 100%;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  flex-wrap: wrap;
  position: relative; line-height: 60rpx; border-bottom: 1px solid #eee
}
.content{ width: 100%;
  box-sizing: border-box;
  align-items: center;
  flex-wrap: wrap;
  position: relative; line-height: 60rpx; border-bottom: 1px solid #eee}
.list{ width: 100%;
  box-sizing: border-box;
  align-items: center;
  flex-wrap: wrap;
  position: relative; line-height: 60rpx; border-bottom: 1px solid #eee}
 .list .list-label{ display: inline-block; vertical-align: top;}
.header-content,
.content-content,
.Addmore {
  font-size: 28rpx; color: #363636;
}
.content-title{font-size: 28rpx;}
.header-label,
.master-label,
.list-label,
.people-label,
.content-title {
  font-size: 30rpx; width: 180rpx; box-sizing: border-box; padding-right: 10rpx; text-align: right
}
.header-content,
.master-name,
.people-number {
  font-size: 28rpx;
}
.add {
  position: absolute;
  top: 20rpx;
  right: 20rpx;
  width: 60rpx;
  height: 60rpx;
}
.add-img {
  width: 100%;
  height: 100%;
}
.content-title {
  font-size: 28rpx; display: inline-block; vertical-align: top
}
.content-content {
  font-size: 28rpx;
}
.href {
  justify-content: space-around;
}
.href-friends,
.href-detail {
  color: bule;
  text-decoration: underline;
}
.nav_section_items{
   /* display:flex;*/
   text-align: center;
    flex-direction: row;
    justify-content: space-between;
    padding:30rpx 0%;
   
    position: relative;
    width: 33%; float: left;
   
}
.delect_list{ position: absolute; font-size: 24rpx; top: 15rpx; right: 15rpx;}
.delect_list image{ width: 40rpx; height: 40rpx;}
.nav_section_items:active{
    background:#ddd;
}
.nav_section_items .section_image{
    width:100%;
    height:158rpx;display: block
}
.nav_section_items .section_image image{
    width:140rpx;
    height:140rpx;
    border-radius: 140rpx;
}
.nav_section_items .section_cont{
    width:100%;
    font-size:26rpx;
     margin-top:6px;
    color:#a9a9a9;
    
}
.nav_section_items .section_cont view{
    overflow:hidden;
    text-overflow: ellipsis;
    white-space:nowrap;
    display: block;
}
.nav_section_items .section_cont .section_cont_intro{
    white-space:normal;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}
.nav_section_items .section_cont .section_cont_sub{
    font-size: 30rpx;
    line-height:60rpx;
    color:#000;
    margin-bottom:10rpx;
}
.Position{ color: #333; line-height: 1}
.nav_section_items .section_cont .section_cont_name{
    font-size:24rpx;
    color:#fff;
    background: #f4625c;
    border-radius: 18px;
    width: 70%; margin: 0 11%; padding: 0 2%;
    position: relative;
    top: -6px;  line-height: 45rpx; height: 45rpx;
}
.nav_section_items .section_cont .section_cont_name .icon_vip_img{
   position: absolute; top: -5px; right: -10px; width: 16px; height: 16px;
}
.nav_section_items .section_cont .section_cont_name .icon_vip_img img{ width: 100%; height: 100%}
.nav_section_items .section_book{
  display: none;
    width:80rpx;
    height:80rpx;
    font-size:26rpx;
    border-radius:100%;
    background:#d24a58;
    line-height: 80rpx;
    color:#fff;
    text-align: center;
    margin-top:35rpx;
}
.select_box{
  background: #fff;
  width: 76%;
  margin: -4rpx auto;
  position: relative;
}
.select{
  box-sizing: border-box;
  width: 100%;
  height: 70rpx;
  border:1px solid #ccc;
  border-radius: 8rpx;
  display: flex;
  align-items: center;
  padding: 0 20rpx;
}
.select_text{
  font-size: 30rpx;
  flex: 1;
}
.select_img{
  width: 25rpx;
  height: 20rpx;
  display: block;
  transition:transform 0.3s;
}
.list-scroll {
  position: relative;
  padding-bottom: 100rpx;
}
.set {
  position: absolute;
  top: 4rpx;
  right: 2%;
}
.list_p{ width: 75%; display: inline-block; vertical-align: top}
.exit {
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.exit-group {
  width: 80%;
  font-size: 28rpx;
  height: 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 10rpx;
  color: #fff;
  background-color: #e02e24;
}
.content {
  margin-bottom: 10rpx;
}
.set-btn {
  font-size: 24rpx;
  height: 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 10rpx;
  color: #fff;
  background-color: #e02e24;
  padding: 0 30rpx;
}
.content-content{ width: 74%; display: inline-block; vertical-align: top}
.add_but{ width: 50%; text-align: center; font-size: 27rpx; display: inline-block; vertical-align: bottom; height: 70rpx; margin: 10rpx 0;}
.creat_q{ width: 50%; text-align: center; font-size: 27rpx; display: inline-block; vertical-align: bottom; height: 70rpx; margin: 10rpx 0}