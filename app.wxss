/**app.wxss**/
/*轮播*/
.swiper-container{
 position: relative;
}
/* .swiper-container .swiper{
 height: 300rpx;
} */
.swiper-container .swiper .img{
 width: 100%;
 height: 100%;
}
.swiper-container .dots{
 position: absolute;
 left: 0;
 right: 0;
 display: flex;
 justify-content: center;
}
.swiper-container .dots .dot{
 margin: 0 8rpx;
 width: 14rpx;
 height: 14rpx;
 background: #fff;
 border-radius: 8rpx;
 transition: all .6s;
}
.swiper-container .dots .dot.active{
 width: 14rpx;
 background: #f80;
}





.swipercontent {
  position: relative;
}

swiper{
  width: 100%;
  height:340rpx;
}

swiper image {
  display: block;
  width: 100%;
  height: 100%;
}

.dots{
  position: absolute;
  left: 0;
  right: 0;
  bottom: 20rpx;
  display: flex;
  justify-content: center;
}

.dot{
  margin: 0 8rpx;
  width: 16rpx;
  height: 16rpx;
  background: #fff;
  border-radius: 8rpx;
  transition: all .6s;
}
.dot.active{
  width: 16rpx;
  background:#E02E24;
}
/*轮播end*/

page{
  display:flex;
  flex-direction:column;
  height:100%;
}
.container {
  display: flex;
  flex-direction: column;
  /*align-items: center;*/
  justify-content: space-between;
  box-sizing: border-box;
  font-family:'微软雅黑';
} 
/* 滚动条 */
.page-body {
  display: flex;
  flex: 1;
  flex-direction: column;
}
/* 提示语 */
.tips {
  font-size: 28rpx;
  text-align: center;
  padding: 50rpx;
  color: #ccc;
  display:none;
}

.tips image {
  width: 40rpx;
  height: 40rpx;
  margin-right: 20rpx;
}

.tips image,
.tips text {
  vertical-align: middle;
}

/*tip hide*/
.tip_0,.tip_1,tip_2,tip3{
    display:block;
}
.q_color{ color: #969696 !important; font-size: 28rpx;}
.bot_v{position: fixed; bottom: 0; left: 0; z-index: 9999; background: #fff; width: 100%; height: 90rpx;}
.tj{ width: 90%; height: 70rpx; line-height: 70rpx; text-align: center; background: #e02e24 !important; color: #fff; margin: 0 5% 20rpx 5%; font-size: 28rpx; border-radius: 10rpx;}
.line_1{ overflow:hidden;text-overflow:ellipsis;  white-space:nowrap;}
.line_2{  white-space:normal; display: -webkit-box; -webkit-line-clamp: 2; -webkit-box-orient: vertical; overflow: hidden}
.line_3{  white-space:normal; display: -webkit-box; -webkit-line-clamp: 3; -webkit-box-orient: vertical; overflow: hidden}
.line_4{  white-space:normal; display: -webkit-box; -webkit-line-clamp: 4; -webkit-box-orient: vertical; overflow: hidden}
.lin_x{ width: 100%; height: 16rpx ; background: #f5f5f5; display: block}
 .line{ position: absolute; left: 16rpx; width: 6rpx; border-radius: 10rpx; height: 26rpx; top: 22rpx; background: #e02e24}

.no_any{ width: 100%; text-align: center; padding: 15% 0}
.no_any .img{ width: 202rpx; height: 120rpx;}
.no_any .text{ font-size: 28rpx; color: #363636}
.no_any_pic .img {width:280rpx;height:326rpx; margin-bottom: 20rpx}
.no_any_pic .text{ color: #666666; font-size: 26rpx;}


.no_else{ text-align: center; font-size: 24rpx; color: #363636}
.no_else .img{ width: 16rpx; height: 16rpx;}


.radio{ width: 180rpx; position: relative; padding-left: 60rpx;  line-height: 60rpx; height: 60rpx}
.radio_v{ position: absolute; left: 8rpx; top: -1rpx}
.radio .text{ line-height: 30rpx}
.company_box{ text-align: center}
.company_box .text{ display: inline-block; width: 48%; vertical-align: middle;box-sizing: border-box}
.company_box .name{ font-size: 24rpx; padding-right: 30rpx; text-align: right; }
.DemandIndustry{ padding-left: 30rpx; text-align: left }
.DemandIndustry text{background: #FFC047; color: #ffffff; border-radius: 10rpx; font-size: 24rpx; padding: 4rpx 14rpx;}
.company_box .lin_in{ width: 2%; padding: 0; text-align: center; color: #D6D6D6; display: inline-block; vertical-align: middle; font-size: 28rpx}
.red_font{ color: #E42822 !important}
.line2{  white-space:normal; display: -webkit-box; -webkit-line-clamp: 2; -webkit-box-orient: vertical; overflow: hidden}
.line3{  white-space:normal; display: -webkit-box; -webkit-line-clamp: 3; -webkit-box-orient: vertical; overflow: hidden}
picker{ color: #999;font-size: 28rpx !important; display: inline-block; vertical-align: top; line-height: 85rpx }
.pick_list{ display: inline-block; vertical-align: bottom;line-height:86rpx; text-indent: 14rpx}

/*  重写 radio 样式  */
/* 未选中的 背景样式 */
radio .wx-radio-input{
   border-radius: 50%;/* 圆角 */
   width: 38rpx;
   height: 38rpx;
 
}
/* 选中后的 背景样式 （红色背景 无边框 可根据UI需求自己修改） */
radio .wx-radio-input.wx-radio-input-checked{
   border: none;
   background: red !important;
}
/* 选中后的 对勾样式 （白色对勾 可根据UI需求自己修改） */
radio .wx-radio-input.wx-radio-input-checked::before{
   border-radius: 50%;/* 圆角 */
   width: 40rpx; /* 选中后对勾大小，不要超过背景的尺寸 */
   height: 40rpx; /* 选中后对勾大小，不要超过背景的尺寸 */
   line-height: 40rpx;
   text-align: center;
   font-size:30rpx; /* 对勾大小 30rpx */
   color:#fff; /* 对勾颜色 白色 */
   background: transparent;
   transform:translate(-50%, -50%) scale(1);
   -webkit-transform:translate(-50%, -50%) scale(1);
}
